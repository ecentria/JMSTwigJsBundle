{
    "name": "jms/twig-js-bundle",
    "type": "symfony-bundle",
    "description": "Integrates twig.js into your Symfony2 application",
    "keywords": [
        "templating"
    ],
    "homepage": "http://jmsyst.com/bundles/JMSTwigJsBundle",
    "license": "Apache2",
    "authors": [
        {
            "name": "Johannes M. Schmitt",
            "email": "schmittjoh@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "github",
            "no-api": true,
            "url": "https://github.com/ecentria/twig.js.git"
        }
    ],
    "require": {
        "php": ">=7.3",
        "jms/twig-js": "dev-twig-2.x|dev-twig-2-5548-fixes",
        "symfony/http-kernel": "^3.0|^4.0|^5.0",
        "symfony/console": "^3.0|^4.0|^5.0",
        "symfony/dependency-injection": "^3.0|^4.0|^5.0",
        "symfony/config": "^3.0|^4.0|^5.0",
        "symfony/translation": "^3.0|^4.0|^5.0",
        "symfony/twig-bridge": "^3.0|^4.0|^5.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "JMS\\TwigJsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JMS\\TwigJsBundle\\Tests\\": "tests/"
        }
    }
}
